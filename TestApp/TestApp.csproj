<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>Exe</OutputType>
		<TargetFramework>net7.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<AllowUnsafeBlocks>true</AllowUnsafeBlocks>
		<AnalysisLevel>latest-all</AnalysisLevel>
		<EnableTrimAnalyzer>true</EnableTrimAnalyzer>
		<EnablePreviewFeatures>True</EnablePreviewFeatures>
		<Configurations>Debug;Release;TraceLeak</Configurations>
	</PropertyGroup>

	<ItemGroup>
		<Compile Remove="Format.cs" />
		<Compile Remove="FormatSilk.cs" />
		<Compile Remove="SPIRV.Commands.cs" />
		<Compile Remove="SPIRV.Enumerations.cs" />
	</ItemGroup>

	<ItemGroup>
		<None Include="FormatSilk.cs">
		  <CopyToOutputDirectory>Always</CopyToOutputDirectory>
		</None>
		<None Include="Format.cs">
		  <CopyToOutputDirectory>Always</CopyToOutputDirectory>
		</None>
		<None Include="SPIRV.Commands.cs">
			<CopyToOutputDirectory>Always</CopyToOutputDirectory>
		</None>
	</ItemGroup>

	<ItemGroup>
		<None Update="env_o.dds">
			<CopyToOutputDirectory>Always</CopyToOutputDirectory>
		</None>
	</ItemGroup>



	<ItemGroup>
	  <None Include="SPIRV.Enumerations.cs">
	    <CopyToOutputDirectory>Always</CopyToOutputDirectory>
	  </None>
	</ItemGroup>



</Project>
